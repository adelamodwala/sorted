<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

  <description>The Sorted object definitions for the DAO tier.</description>

  <object id="SorterDAO" type="Sorted.com.sorted.dao.implementation.SorterDAOImpl">
    <description>The action class that handles the services for sorters.</description>
    <property name="availableSorters" ref="AvailableSorters" />
  </object>

  <object id="AvailableSorters" type="Sorted.com.sorted.utils.ISorter[]">
    <constructor-arg value="3" />
    <property name="[0]" ref="BubbleSorter" />
    <property name="[1]" ref="InsertSorter" />
    <property name="[2]" ref="SelectionSorter" />
  </object>

  <object id="BubbleSorter" type="Sorted.com.sorted.utils.implementation.SorterBubble" />
  <object id="InsertSorter" type="Sorted.com.sorted.utils.implementation.SorterInsert" />
  <object id="SelectionSorter" type="Sorted.com.sorted.utils.implementation.SorterSelection" />

</objects>